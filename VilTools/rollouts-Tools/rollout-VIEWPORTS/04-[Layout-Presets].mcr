

macroscript layout_restore_layout_1
category:   "_Viewports-Layout-Preset"
buttontext: "Layout 1"
tooltip:    "Restore Layout 1\n\nCTRL: Reset Viewport Spliters"
icon:       "images: #('/Icons/Layouts.bmp',undefined,14, 1)|MENU:true|height:28|across:14|offset:[0,4]"
(
	(ViewportLayoutManager_v()).restoreLayout(#layout_1) reset_splitters: keyboard.controlPressed
)

